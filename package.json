{
  "name": "@aliexme/eslint-config",
  "version": "0.0.0",
  "description": "Rule set for ESLint",
  "private": false,
  "author": "Alexander Smirnov <al.smirnov996@gmail.com>",
  "homepage": "https://github.com/aliexme/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliexme/eslint-config"
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "files": [
    "./index.js",
    "./rules/**"
  ],
  "exports": {
    ".": "./index.js",
    "./base": "./rules/base.js",
    "./import": "./rules/import.js",
    "./typescript": "./rules/typescript.js"
  },
  "scripts": {
    "prepare": "husky install",
    "lint:check": "eslint --ext .js,.mjs,.ts --ignore-path .gitignore --cache ./",
    "lint:fix": "pnpm lint:check --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": ">=8",
    "eslint-plugin-import": "^2.27.5"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": { "optional": true },
    "@typescript-eslint/parser": { "optional": true }
  }
}
